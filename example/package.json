    "lint": "vue-cli-service lint",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "dependencies": {
    "@antoinemairesse/vuex-crud": "^1.0.2",
    "axios": "^1.4.0",
